# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-service-role-key  # Use Service Role Key for backend operations
SUPABASE_ANON_KEY=your-supabase-anon-key     # For frontend Supabase client

# LLM Configuration
# Uncomment and set one of these based on your LLM provider
# OPENAI_API_KEY=your-openai-key
# ANTHROPIC_API_KEY=your-anthropic-key
# COHERE_API_KEY=your-cohere-api-key

# LangSmith Configuration (for agent deployment and monitoring)
# LANGSMITH_API_KEY=your-langsmith-api-key
# LANGSMITH_PROJECT=interview-evaluator
# LANGSMITH_TRACING_V2=true
# LANGCHAIN_TRACING_V2=true

# Application Settings
LOG_LEVEL=INFO
MAX_FILE_SIZE_MB=5

# Monitoring & Error Tracking
SENTRY_DSN=your-sentry-backend-dsn
SENTRY_ENVIRONMENT=production
SENTRY_TRACES_SAMPLE_RATE=0.2  # Adjust based on traffic volume

# Frontend Settings (add to frontend/.env file)
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
# VITE_SENTRY_DSN=your-sentry-frontend-dsn
# VITE_SENTRY_ENVIRONMENT=production
# SENTRY_AUTH_TOKEN=your-sentry-auth-token  # For uploading source maps